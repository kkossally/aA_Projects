<h1>Selected Cat</h1>
<h4><%= @cat.name %></h4>
<table>
  <thead>
    <tr>
      <th>Birthdate</th>
      <th>Color</th>
      <th>Sex</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @cat.birth_date %></td>
      <td><%= @cat.color %></td>
      <td><%= @cat.sex %></td>
      <td><%= @cat.description %></td>
    </tr>
  </tbody>
</table>
<br>
<a href="<%= edit_cat_url(@cat) %>">edit cat</a> <br>
<a href="<%= cats_url %>">see all cats</a>
